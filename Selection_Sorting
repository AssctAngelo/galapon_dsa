import java.io.*;

public class Main
{

  void sort (int array[])
  {

    int r = array.length;



    for (int i = 0; i < r - 1; i++)

      {


	int min_value = i;

	for (int j = i + 1; j < r; j++)

	  if (array[j] < array[min_value])

	      min_value = j;



	int temp = array[min_value];

	  array[min_value] = array[i];

	  array[i] = temp;

      }
  }


  void printArray (int array[])
  {

    int r = array.length;

    for (int i = 0; i < r; ++i)

      System.out.print (array[i] + " ");

    System.out.println ();

  }


  public static void main (String args[])
  {

    Main srt = new Main ();

    int array[] = { 32, 16, 18, 22, 10 };

    srt.sort (array);

    srt.printArray (array);

  }
}
